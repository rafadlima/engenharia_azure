{
	"name": "Superstore Datalake",
	"properties": {
		"activities": [
			{
				"name": "Copy Superstore",
				"description": "Carga de Dados do CSV Superstore Marketing Campaign Dataset",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Superstoredata",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DBSuperstoredata",
						"type": "DatasetReference",
						"parameters": {
							"tableSchema": "dbo",
							"tableName": "stgsuperstore"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Superstore Pipeline"
		},
		"annotations": [],
		"lastPublishTime": "2023-01-05T21:12:08Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}